{
  "name": "advancemais-api",
  "version": "1.0.0",
  "description": "API do sistema AdvanceMais - Gestão de usuários e autenticação",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean",
    "postbuild": "pnpm run prisma:generate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "db:setup": "pnpm run prisma:push && pnpm run prisma:generate",
    "lint": "echo \"Linting not configured yet\"",
    "format": "echo \"Formatting not configured yet\""
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "prisma",
    "supabase",
    "jwt",
    "authentication"
  ],
  "author": "Revolck",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "cross-env": "^7.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@getbrevo/brevo": "^2.5.0",
    "@prisma/client": "^6.13.0",
    "@supabase/supabase-js": "^2.53.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "mercadopago": "^2.8.0",
    "prisma": "^6.13.0",
    "zod": "^4.0.13"
  }
}
