{
  "name": "advancemais-api",
  "version": "3.0.3",
  "description": "API do sistema Advance+ - Gestão de usuários e autenticação",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --require tsconfig-paths/register src/index.ts",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "build": "tsc",
    "build:prod": "pnpm prisma db push --accept-data-loss && pnpm prisma generate && pnpm run build",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean",
    "postbuild": "echo 'Build completed'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest src/modules/cursos/__tests__",
    "test:performance": "jest src/modules/cursos/__tests__/performance.test.ts",
    "test:api": "./scripts/test-api.sh",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:push:force": "prisma db push --accept-data-loss",
    "prisma:pull": "prisma db pull",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:reset:manual": "ts-node --transpile-only -r tsconfig-paths/register scripts/reset-database.ts",
    "prisma:reset:complete": "ts-node --transpile-only -r tsconfig-paths/register scripts/reset-database-complete.ts",
    "prisma:reset:prod": "pnpm run prisma:reset:complete && pnpm run prisma:push && pnpm run prisma:generate",
    "prisma:fix:migration": "ts-node --transpile-only -r tsconfig-paths/register scripts/fix-migration-state.ts",
    "prisma:studio": "prisma studio",
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "prisma:seed": "pnpm run seed",
    "db:setup": "pnpm run prisma:push && pnpm run prisma:generate",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "prisma",
    "supabase",
    "jwt",
    "authentication"
  ],
  "author": "Revolck",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "lru-cache": "^11.2.1",
    "node-cron": "^3.0.3",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@getbrevo/brevo": "^2.5.0",
    "@prisma/client": "^6.19.0",
    "@supabase/supabase-js": "^2.57.4",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "mercadopago": "^2.9.0",
    "multer": "^2.0.2",
    "pino": "^9.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.25.76"
  }
}
